{
  "name": "wyin-api-feed",
  "version": "0.1.0",
  "description": "REST API serving historical events",
  "main": "dist/src/server.js",
  "author": "mrl5",
  "license": "MPL-2.0",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || echo 'Could not set git hooks'",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "recommended-bump": "conventional-recommended-bump -p angular",
    "build": "yarn run tsoa && tsc",
    "tsoa": "tsoa spec-and-routes",
    "start": "node dist/src/server.js",
    "dev": "concurrently 'nodemon' 'nodemon -x tsoa spec-and-routes'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix && tsc --noEmit --pretty",
    "lint:ci": "eslint '*/**/*.{js,ts,tsx}' --max-warnings=0 && tsc --noEmit --pretty",
    "lint:commit": "commitlint"
  },
  "dependencies": {
    "@spio-wyin/wyin-sdk-feed": "^2.0.0",
    "@tsoa/runtime": "^3.13.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "swagger-ui-express": "^4.1.6",
    "ts-custom-error": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@tsconfig/node14": "^1.0.1",
    "@tsoa/cli": "^3.14.0",
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/node": "^14.14.31",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^5.3.0",
    "concurrently": "^6.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-recommended-bump": "^6.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
